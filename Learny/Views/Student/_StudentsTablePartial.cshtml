@model IEnumerable<Learny.Models.ApplicationUser>

<table class="table">
    <tr class="headerRow">
        <th>             Deltagare       </th>
        <th>            @Html.DisplayNameFor(model => model.Email)        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>                 @Html.ActionLink(item.Name, "Details", new { email = item.Email })                           </td>
            <td>                @Html.DisplayFor(modelItem => item.Email)            </td>
        </tr>
    }

</table>


